{
  "name": "whatsapp-bimcat-electron",
  "version": "3.0.0",
  "description": "Aplicación Electron para automatización de WhatsApp con extracción de números y envío de mensajes",
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder --config electron-builder-config.js",
    "build:win": "electron-builder --win --config electron-builder-config.js",
    "build:mac": "electron-builder --mac --config electron-builder-config.js",
    "build:linux": "electron-builder --linux --config electron-builder-config.js",
    "build:clean": "node build.js",
    "build:safe": "node build-safe.js",
    "build:improved": "node build-improved.js",
    "build:simple": "node build-simple.js",
    "build:installer": "node build-installer.js",
    "test-build": "node test-build.js",
    "verify-build": "node verify-build.js",
    "postinstall": "electron-builder install-app-deps",
    "test-server": "node test-server.js"
  },
  "keywords": [
    "whatsapp",
    "electron",
    "automation",
    "bimcat"
  ],
  "author": "Bimcat Team",
  "license": "ISC",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.6.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "multer": "^1.4.5-lts.2",
    "qrcode-terminal": "^0.12.0",
    "whatsapp-web.js": "^1.31.0",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3",
    "dotenv": "^16.3.1",
    "electron-store": "^8.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
